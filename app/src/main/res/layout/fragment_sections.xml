<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.ui.main.sections.SectionsFragment">

    <com.ml.truckingandconstructionwork.presentation.custom_view.toolbar.ToolbarStandard
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="@string/sections"
        app:leftItemEnabled="true"
        app:leftItem="@drawable/ic_arrow_back_black" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">



    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/trucks"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/trucks"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trucks"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/dump_trucks"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/dump_truck"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trucks"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dump_trucks"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/truck_cranes"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/truck_cranes"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dump_trucks"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/truck_cranes"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/manipulators"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/manipulators"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/truck_cranes"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/manipulators"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/crawler_excavators"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/crawler_excavators"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/manipulators"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/crawler_excavators"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/wheeled_excavators"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/wheeled_excavators"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/crawler_excavators"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/wheeled_excavators"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/excavator_loaders"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/excavator_loaders"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/wheeled_excavators"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line7"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/excavator_loaders"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/concrete_mixers"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/concrete_mixers"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/excavator_loaders"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line8"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/concrete_mixers"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/concrete_pump"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/concrete_pump"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/concrete_mixers"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line9"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/concrete_pump"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tow_trucks"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50"
        android:background="?selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/size_15"
        android:drawableEnd="@drawable/ic_next_gray"
        android:text="@string/tow_trucks"
        android:layout_marginHorizontal="@dimen/size_15"
        android:drawablePadding = "@dimen/size_10"
        android:textSize="@dimen/t_size_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/concrete_pump"
        tools:ignore="RtlSymmetry" />

    <View
        android:id="@+id/line10"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:background="@color/ptGray3"
        android:layout_marginHorizontal="@dimen/size_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tow_trucks"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>